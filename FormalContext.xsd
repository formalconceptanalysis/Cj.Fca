  <xsd:element name='Data'>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name='Header' minOccurs='1' maxOccurs='1'>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:choice>
                <xsd:element name='Identifier' minOccurs="1">
                  <xsd:simpleType>
                    <xsd:annotation>
                      <xsd:documentation xml:lang='en-us'>Dot separated identifiers where each identifier must comprise only ASCII alphanumerics and hyphens.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                      <xsd:pattern value="^(?:([0-9a-zA-Z-]+(?:\.[0-9a-zA-Z-]+)*))?$"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:element>
                <xsd:element name="Title" type='xsd:string' minOccurs="1"/>
              </xsd:choice>
              <xsd:element name='Subtitle' minOccurs='0' maxOccurs='1' type='xsd:string'/>
              <xsd:element name='Memo' type='fca:MemoType' minOccurs='0' maxOccurs='1'/>
              <xsd:element name='Reference' type='fca:ReferenceType' minOccurs='0' maxOccurs='1'/>
              <xsd:element name='Copyrights' type='fca:CopyrightType' minOccurs='0' maxOccurs='1'/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name='FormalContext' minOccurs='1' maxOccurs='1'>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name='Status' minOccurs='0' maxOccurs='1'>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name='Swapped' minOccurs='0' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:attribute name='ByObject' type='xsd:boolean' use='required'/>
                        <xsd:attribute name='ByAttribute' type='xsd:boolean' use='required'/>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:choice>
                      <xsd:element name='PreservedByObject' minOccurs='0' maxOccurs='1'>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name='ReIndexedByClarifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                            <xsd:element name='ReIndexedByPurifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                            <xsd:element name='ReIndexedByReducing' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                            <xsd:element name='ReIndexedByTrimming' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                          </xsd:sequence>
                          <xsd:attribute name='MarkerMinExclusive' use='required'>
                            <xsd:simpleType>
                              <xsd:restriction base='xsd:int'>
                                <xsd:minExclusive value="1"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name='PreservedByAttribute' minOccurs='0' maxOccurs='1'>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name='ReIndexedByClarifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                            <xsd:element name='ReIndexedByPurifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                            <xsd:element name='ReIndexedByReducing' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                            <xsd:element name='ReIndexedByTrimming' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                          </xsd:sequence>
                          <xsd:attribute name='MarkerMinExclusive' use='required'>
                            <xsd:simpleType>
                              <xsd:restriction base='xsd:int'>
                                <xsd:minExclusive value="1"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:complexType>
                      </xsd:element>
                    </xsd:choice>
                    <xsd:element name='Clarified' minOccurs='0' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:attribute name='ByObject' type='xsd:boolean' use='required'/>
                        <xsd:attribute name='ByAttribute' type='xsd:boolean' use='required'/>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name='Purified' minOccurs='0' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:attribute name='ByObject' type='xsd:boolean' use='required'/>
                        <xsd:attribute name='ByAttribute' type='xsd:boolean' use='required'/>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name='Reduced' minOccurs='0' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:attribute name='ByObject' type='xsd:boolean' use='required'/>
                        <xsd:attribute name='ByAttribute' type='xsd:boolean' use='required'/>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name='Trimmed' minOccurs='0' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:attribute name='ByObject' type='xsd:boolean' use='required'/>
                        <xsd:attribute name='ByAttribute' type='xsd:boolean' use='required'/>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name='Declarations' minOccurs='1' maxOccurs='1'>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name='Attributes' minOccurs='1' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name='Reduced' minOccurs='0' maxOccurs='1'/>
                          <xsd:element name='Trimmed' minOccurs='0' maxOccurs='1'/>
                          <xsd:element name='Attribute' minOccurs='1' maxOccurs='unbounded'>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name='Term' type='xsd:string' minOccurs='0' maxOccurs='1'/>
                                <xsd:element name='Source' type='fca:SourceType' minOccurs='0' maxOccurs='1'/>
                                <xsd:element name='Memo' type='fca:MemoType' minOccurs='0' maxOccurs='1'/>
                                <xsd:element name='ReIndexedBySwapping' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='Clarified' minOccurs='0' maxOccurs='1'>
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name='Attribute' minOccurs='1' maxOccurs='unbounded'>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name='Term' type='xsd:string' minOccurs='0' maxOccurs='1'/>
                                            <xsd:element name='Source' type='fca:SourceType' minOccurs='0' maxOccurs='1'/>
                                            <xsd:element name='Memo' type='fca:MemoType' minOccurs='0' maxOccurs='1'/>
                                            <xsd:element name='ReIndexedBySwapping' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                          </xsd:sequence>
                                          <xsd:attributeGroup ref='fca:DeclarationGroup'/>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name='ReIndexedByClarifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='ReIndexedByPurifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='ReIndexedByReducing' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='ReIndexedByTrimming' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                              </xsd:sequence>
                              <xsd:attributeGroup ref='fca:DeclarationGroup'/>
                              <xsd:attribute name='_1-generated' use='optional'>
                                <xsd:simpleType>
                                  <xsd:restriction base='xsd:string'>
                                    <xsd:pattern value='{(.*?)}'/>
                                  </xsd:restriction>
                                </xsd:simpleType>
                              </xsd:attribute>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name='Objects' minOccurs='1' maxOccurs='1'>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name='Reduced' minOccurs='0' maxOccurs='1'/>
                          <xsd:element name='Trimmed' minOccurs='0' maxOccurs='1'/>
                          <xsd:element name='Object' minOccurs='1' maxOccurs='unbounded'>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name='Term' type='xsd:string' minOccurs='0' maxOccurs='1'/>
                                <xsd:element name='Source' type='fca:SourceType' minOccurs='0' maxOccurs='1'/>
                                <xsd:element name='Memo' type='fca:MemoType' minOccurs='0' maxOccurs='1'/>
                                <xsd:element name='ReIndexedBySwapping' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='Clarified' minOccurs='0' maxOccurs='1'>
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name='Object' minOccurs='1' maxOccurs='unbounded'>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name='Term' type='xsd:string' minOccurs='0' maxOccurs='1'/>
                                            <xsd:element name='Source' type='fca:SourceType' minOccurs='0' maxOccurs='1'/>
                                            <xsd:element name='Memo' type='fca:MemoType' minOccurs='0' maxOccurs='1'/>
                                            <xsd:element name='ReIndexedBySwapping' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                          </xsd:sequence>
                                          <xsd:attributeGroup ref='fca:DeclarationGroup'/>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name='ReIndexedByClarifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='ReIndexedByPurifying' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='ReIndexedByReducing' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                                <xsd:element name='ReIndexedByTrimming' type='xsd:positiveInteger' minOccurs='0' maxOccurs='unbounded'/>
                              </xsd:sequence>
                              <xsd:attributeGroup ref='fca:DeclarationGroup'/>
                              <xsd:attribute name='_0-generated' use='optional'>
                                <xsd:simpleType>
                                  <xsd:restriction base='xsd:string'>
                                    <xsd:pattern value='{(.*?)}'/>
                                  </xsd:restriction>
                                </xsd:simpleType>
                              </xsd:attribute>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name='Items' minOccurs='1' maxOccurs='1'>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name='Reduced' minOccurs='0' maxOccurs='unbounded'>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name='Attributes' minOccurs='0' maxOccurs='1'>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name='Attribute' type='fca:ReducibleItemType' minOccurs='1' maxOccurs='unbounded'/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name='Objects' minOccurs='0' maxOccurs='1'>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name='Object' type='fca:ReducibleItemType' minOccurs='1' maxOccurs='unbounded'/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name='Item' type='fca:ContextItemType' minOccurs='1' maxOccurs='unbounded'/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute name='Status' use='optional'>
              <xsd:simpleType>
                <xsd:restriction base='xsd:string'>
                  <xsd:enumeration value='Canonical'/>
                  <xsd:enumeration value='Standard'/>
                  <xsd:enumeration value='One-element'/>
                  <xsd:enumeration value='Preserved'/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name='Schema' default='1.0.0' use='optional'>
        <xsd:annotation>
          <xsd:documentation xml:lang='en-us'>
            Versioning according to https://semver.org/
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base='xsd:token'>
            <xsd:pattern value='^(0|[1-9]\d*)\.(0|[1-9]\d*)\.(0|[1-9]\d*)(?:-((?:0|[1-9]\d*|\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\.(?:0|[1-9]\d*|\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\+([0-9a-zA-Z-]+(?:\.[0-9a-zA-Z-]+)*))?$'>
            </xsd:pattern>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name ='ReferenceType'>
    <xsd:annotation>
      <xsd:documentation xml:lang='en-us'>Format of author-date reference according to The Chicago Manual of Style.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name='Document' type='xsd:string' minOccurs='1' maxOccurs='1'/>
      <xsd:element name='Copyrights' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='Copyright' type='fca:CopyrightType' minOccurs='1' maxOccurs='unbounded'/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name ='ReferenceLinkType'>
    <xsd:sequence>
      <xsd:element name='See' type='xsd:string' minOccurs='1' maxOccurs='1'/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='CopyrightType'>
    <xsd:sequence>
      <xsd:element name='Symbol' type='xsd:string' minOccurs='1' maxOccurs='1' default='©'/>
      <xsd:element name='FirstPublished' type='xsd:gYear' minOccurs='1' maxOccurs='1'/>
      <xsd:element name='Owners' minOccurs='1' maxOccurs='unbounded'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='Name' type='xsd:string' minOccurs='1' maxOccurs='unbounded'/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='Content' type='xsd:string' minOccurs='1' maxOccurs='1'/>
      <xsd:element name='CanonicalURL' type='xsd:string' minOccurs='0' maxOccurs='unbounded'/>
      <xsd:element name='Changes' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='Change' type='xsd:string' minOccurs='1' maxOccurs='unbounded'/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='MemoType' mixed='true'>
    <xsd:sequence>
      <xsd:element name='Text' type='xsd:string' minOccurs='0' maxOccurs='1'/>
    </xsd:sequence>
    <xsd:attribute name='Text' type='xsd:string' use='optional'/>
  </xsd:complexType>
